	#Cotton Plantation
		add_1_crop_farm_in_random_state = {
			random_owned_controlled_state = {
				limit = {
					set_temp_variable = { industry_token_temp = token:crop_farm }
					has_free_industry_slots = yes
				}
				set_temp_variable = { industry_token = token:crop_farm }
				set_temp_variable = { x_temp = 1 }
				construct_x_industries = yes
			}
		}
